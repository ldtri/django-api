server {
    listen       80;
    charset      utf-8;

    client_max_body_size 100M;

    location / {
        add_header 'Access-Control-Allow-Origin' '$http_origin';
        include       uwsgi_params;
        uwsgi_pass    127.0.0.1:3031;
    }

    location /media/photos {
        alias /opt/uploaded-files/photos/;
    }
}
